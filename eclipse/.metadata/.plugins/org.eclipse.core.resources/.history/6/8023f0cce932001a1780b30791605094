package net.minecraft.src;

public enum EnumBowType {
	DEFAULT(384,2,0f,1.5f),
	IRONBOW(608,4, 0.2f,1.1f),
	DIAMONDBOW(816,5,0.3f, 0.8f),
	GOLDBOW(512,3,0.6f, 0.1f);
	
private EnumBowType(int durability, int damage, float velocity, float accuracy)
{
	this.durability = durability;
	this.addedDmg = damage;
	this.velocity = velocity;
}
public int durability;
public int addedDmg;
public float velocity;
public float accuracy;
}


